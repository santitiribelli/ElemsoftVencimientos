@using Entities
@model BuquesViajesClientesServiciosDetallesMedicos
<form class="form-horizontal rowAlt" role="form" id="formDetallesMedicos">
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <div class="col-lg-12">
