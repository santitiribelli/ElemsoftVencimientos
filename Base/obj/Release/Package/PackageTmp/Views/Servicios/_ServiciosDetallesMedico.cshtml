@if (Model != null)
{
<script>
    $(function () {
        $("#fechaDesde").val('@Model.BuqViaCliSerDetMed_FechaDesde.ToString("dd/MM/yyyy")');
        $("#fechaHasta").val('@(Model.BuqViaCliSerDetMed_FechaHasta.HasValue ? Model.BuqViaCliSerDetMed_FechaHasta.Value.ToString("dd/MM/yyyy") : "")');
        $("#proveedor").select2('val', '@Model.Prov_Id');
        $("#tratamiento").val('@Html.Raw(Ajax.JavaScriptStringEncode(Model.BuqViaCliSerDetMed_Tratamiento.ToString()))');
        $("#estado").select2('val', '@Model.Clas_ServicioEstado_Id');
        $("#asistente").select2('val', '@Model.Emp_Id');
        var proveedor = '@(Model.Prov_Id.HasValue ? true : false)';
        if (proveedor == "True")
        {
            $('#estado').prop("disabled", false);
            $("#estado").select2('val', '839');
        }
        else
                $('#estado').prop("disabled", true);
        $("#observacion").val('@Html.Raw(Ajax.JavaScriptStringEncode(Model.BuqViaCliSerDetMed_Obs))');
          var costos = '@Html.Raw(Ajax.JavaScriptStringEncode((Model.BuquesViajesClientesServicios.BuquesViajesClientesServiciosDetallesMedicosCostos.Count > 0).ToString()))';
        if (costos == "True")
            $("#btnCostos").removeClass("btn-danger").addClass("btn-success");
        else
            $("#btnCostos").removeClass("btn-success").addClass("btn-danger");
        var documentos = '@Html.Raw(Ajax.JavaScriptStringEncode((Model.BuquesViajesClientesServicios.BuquesViajesClientesServiciosDocumentos.Count > 0).ToString()))';
        if (documentos == "True")
            $("#btnDocumentos").removeClass("btn-danger").addClass("btn-success");
        else
            $("#btnDocumentos").removeClass("btn-success").addClass("btn-danger");
        $("#btnDocumentos").show();
        var doc = '@(Model.BuqViaCliSerDetMed_ArchivoAdjunto != null ? Model.BuqViaCliSerDetMed_ArchivoAdjunto.ToString() : "" )';
             if (doc != "") {
                $('#divDescargar').show();
                 $('#lblDescargar').show();
                 $('#descargar').attr("href", '@Url.Content("/Uploads/ServiciosDocumentos/")' + doc);
            }
            else {
                $('#lblDescargar').hide();
                $('#divDescargar').hide();
            }
    });
</script>
}
else
{
    <script>
        $(function () {
            $("#fechaDesde").val('@ViewBag.Fecha');
            $("#estado").select2('val', '839');
            $("#btnDocumentos").hide();
        });
    </script>
